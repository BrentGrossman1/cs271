/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    And(a= a, b= notSel, out= aAndOut);
    And(a= b, b= sel, out= bAndOut);
    Not(in= sel, out= notSel);
    Or(a= aAndOut, b= bAndOut, out= out);
}